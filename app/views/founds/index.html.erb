<h1>Listing founds</h1>

<table>
  <tr>
    <th>Desc</th>
    <th>Date</th>
    <th>Location</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @founds.each do |found| %>
  <tr>
    <td><%= found.desc %></td>
    <td><%= found.date %></td>
    <td><%= found.location %></td>
    <td><%= link_to 'Show', found %></td>
    <td><%= link_to 'Edit', edit_found_path(found) %></td>
    <td><%= link_to 'Destroy', found, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

